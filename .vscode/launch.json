    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: current file (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: main.py (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--dataset",
                "cube_small",
                "--seed",
                "1",
                "--lr",
                "1e-3",
                "--objective",
                "B",
                "--model",
                "B",
                "--batch_size",
                "500",
                "--max_iter",
                "100",
                "--viz_name",
                "debug_test_16",
                "--num_workers",
                "0",
                "--train",
                "True",
                "--gamma",
                "0",
                // "--beta",
                // "20",
                "--z_dim",
                "2",
                "--viz_on",
                "False",
                "--C_stop_iter",
                "100",
                "--prefix",
                // "Additional_training",
                "Initial_training", //This should contain no space
                // "Additional_training_auxiliary_ambiguity",
                // "Additional_training_small_gamma",
                "--base",
                "Initial_training"
                // "Additional_training"
            ]
        },
    ]
}