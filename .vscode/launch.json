    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: current file (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: main.py (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--dataset",
                "cube_random_spherical_position",
                "--seed",
                "1",
                "--lr",
                "1e-3",
                "--beta1",
                "0.9",
                "--beta2",
                "0.999",
                "--objective",
                "B",
                "--model",
                "B",
                "--batch_size",
                "64",
                "--max_iter",
                "1.5e4",
                "--viz_name",
                "test_adadelta",
                "--num_workers",
                "0",
                "--train",
                "False",
                "--gamma",
                "5",
                // "--beta",
                // "20",
                "--z_dim",
                "8",
                "--viz_on",
                "False"
            ]
        },
    ]
}